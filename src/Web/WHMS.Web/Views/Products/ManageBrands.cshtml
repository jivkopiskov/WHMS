@model IEnumerable<WHMS.Data.Models.Products.Brand>;

<div>
    <a type="button" class="btn-primary p-2" href="/Products/AddBrand" role="button">Add Brand</a>
</div>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Brand Name</th>>
        </tr>
    </thead>
    <tbody>
        @foreach (var brand in Model)
        {
            <tr>
                <td>@brand.Id</td>
                <td>@brand.Name</td>
            </tr>
        }
    </tbody>
</table>